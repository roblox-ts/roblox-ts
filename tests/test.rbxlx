						for _k, _v in largeButtonProps do
							_attributes[_k] = _v
						end
						for _k, _v in overloadProps do
							_attributes[_k] = _v
						end
						return Roact.createElement("TextButton", _attributes)
					elseif Size == ButtonSize.Small then
						local _attributes = {
							[Roact.Ref] = buttonRef,
						}
						for _k, _v in smallButtonProps do
